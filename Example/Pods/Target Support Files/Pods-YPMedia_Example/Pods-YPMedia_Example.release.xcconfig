ENABLE_BITCODE = NO
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/YPFoundation" "$PODS_CONFIGURATION_BUILD_DIR/YPMedia" "$PODS_CONFIGURATION_BUILD_DIR/YPPermission"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/YPFoundation/YPFoundation.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/YPMedia/YPMedia.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/YPPermission/YPPermission.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "YPFoundation" -framework "YPMedia" -framework "YPPermission"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
